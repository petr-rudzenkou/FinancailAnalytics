    <Color x:Key="CompanyInfo_Result_Color">#333333</Color>
    <SolidColorBrush x:Key="CompanyInfo_Result_Brush" Color="{StaticResource CompanyInfo_Result_Color}"/>

    <Color x:Key="CompanyInfo_Res_End">#E4E4E4</Color>
    <Color x:Key="CompanyInfo_Res_Start">#FFFFFF</Color>

    <LinearGradientBrush x:Key="CompanyInfo_Res_BG" EndPoint="0.5,1" StartPoint="0.5,0">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="{StaticResource CompanyInfo_Res_Start}" Offset="0"/>
                <GradientStop Color="{StaticResource CompanyInfo_Res_End}" Offset="1"/>
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <!--Message box-->
    <SolidColorBrush x:Key="MessageBoxContentBackgroundBrush" Color="#151516" />
    <SolidColorBrush x:Key="MessageBoxContentBorderBrush" Color="#181818" />
    <SolidColorBrush x:Key="MessageBoxTextForegroundBrush" Color="#C2C2C2" />
    <!--/Message box-->

    <SolidColorBrush x:Key="WindowContentBackgroundBrush" Color="#FF242424"/>
    <SolidColorBrush x:Key="WindowContentBorderBrush" Color="#181818"/>

</ResourceDictionary>

